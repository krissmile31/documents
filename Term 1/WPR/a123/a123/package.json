  "scripts": {
    "beautify": "prettier --no-editorconfig --config .prettierrc --ignore-path ../.gitignore --write '**/*.js'",
    "start": "node -r esm -r module-alias/register src/main",
    "start:dev": "nodemon -r esm -r module-alias/register --watch src src/main.js",
