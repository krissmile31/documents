    "esm": "^3.2.25",
    "module-alias": "^2.2.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "mongoose": "^5.7.3",
    "mongoose-sequence": "^5.2.2",
    "validator": "^11.1.0"
  },
  "_moduleAliases": {
    "@": "src"
  }
