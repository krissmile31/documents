<!-- BEGIN: main -->
<div id="fb-root"></div>
<script>
	( function(d, s, id) {
		var js, fjs = d.getElementsByTagName(s)[0];
		if(d.getElementById(id))
			return;
		js = d.createElement(s);
		js.id = id;
		js.src = "//connect.facebook.net/vi_VN/all.js#xfbml=1";
		fjs.parentNode.insertBefore(js, fjs);
	}(document, 'script', 'facebook-jssdk'));

</script>
<div class="fb-like-box" data-href="http://www.facebook.com/{nickfb}" data-width="{width}" data-height="{height}" data-colorscheme="{color}" data-show-faces="{faces}" data-stream="{stream}" data-header="{header}" data-show-border="false"></div>
<!-- END: main -->
